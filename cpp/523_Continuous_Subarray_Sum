class Solution {
public:
    bool checkSubarraySum(vector<int>& nums, int k) {
        int n = nums.size();
        vector<int> dp(nums.begin(), nums.end());
        for(int i=2;i<=n;++i){
            for(int j=n-1;j>=i-1;--j){
                dp[j] = dp[j-1]+nums[j];
                if(k==0&&dp[j]==0)
                    return true;
                if(k&&dp[j]%k==0)
                    return true;
            }
        }
        return false;
    }
};
